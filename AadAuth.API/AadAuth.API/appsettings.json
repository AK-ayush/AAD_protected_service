{
  "AzureAd": {
    // Login URL of Microsoft
    "Instance": "https://login.microsoftonline.com/",

    // Client ID if our API App Registration in AAD.
    "ClientId": "your client id", 

    // Our Tenant ID
    "TenantId": "your tenatn id",

    // Our API App's unique URI as specified during app registraion
    "Audience": "api://<your client id>"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
